{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint","prettier"],
    "rules": {
        "prettier/prettier": ["error",{"endOfLine":"CRLF"}],
        "@typescript-eslint/no-var-requires": "off"
    }
}
